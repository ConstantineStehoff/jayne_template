@import "bootstrap/bootstrap";
@import "bootstrap/bootstrap-responsive";
@import "font-awesome/font-awesome";
@import "semantic/grid";

/* Custom responsive semantic grid */
// Specify the number of columns and set column and gutter widths
$columns: 12;
@media (min-width: 1200px) {
  $column-width: 70;
  $gutter-width: 30;
}
@media (min-width: 980px) and (max-width: 1199px) {
  $column-width: 60;
  $gutter-width: 20;
}
@media (min-width: 768px) and (max-width: 979px) {  
  $column-width: 42;
  $gutter-width: 20;
}
@media (max-width: 767px){
  // Remove the definition below for a pixel-based layout
  $total-width: 100%;
}

.g-span12{@include column(12);}
//Responsive grid
@media (min-width: 980px) {
  .g-span11{@include column(11);}
  .g-span10{@include column(10);}
  .g-span9{@include column(9);}
  .g-span8{@include column(8);}
  .g-span7{@include column(7);}
  .g-span6{@include column(6);}
  .g-span5{@include column(5);}
  .g-span4{@include column(4);}
  .g-span3{@include column(3);}
  .g-span2{@include column(2);}
  .g-span1{@include column(1);}  
}
@media (min-width: 768px) and (max-width: 979px) { 
  .g-span11{@include column(11);}
  .g-span10{@include column(10);}
  .g-span9{@include column(9);}
  .g-span8{@include column(8);}
  .g-span7{@include column(7);}
  .g-span6{@include column(6);}
  .g-span5{@include column(5);}
  .g-span4{@include column(4);}
  .g-span3{@include column(6);}
  .g-span2{@include column(2);}
  .g-span1{@include column(1);}  
}
// Landscape phone to portrait tablet
@media (max-width: 767px) { 
  .g-span11{@include column(12);}
  .g-span10{@include column(12);}
  .g-span9{@include column(12);}
  .g-span8{@include column(12);}
  .g-span7{@include column(12);}
  .g-span6{@include column(12);}
  .g-span4{@include column(12);}
  .g-span3{@include column(12);}
  .g-span2{@include column(12);}
  .g-span1{@include column(12);} 
}
// Landscape phones and down 
@media (max-width: 480px) { 

}
/* END Custom responsive semantic grid */

/* Bootstrap class overrides */
html {
  width: auto !important;
  overflow-x: hidden !important;
}
body {
  width: auto !important;
  overflow-x: hidden !important;
  font-size: $baseFontSize*1.14285714; //16px
}
.btn{
  background-image: none;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
  filter: none;
  border-width: 2px;
}
::-webkit-input-placeholder{color: $grayLight !important;}:-moz-placeholder{color: $grayLight !important;}::-moz-placeholder{color: $grayLight !important;}:-ms-input-placeholder{color: $grayLight !important;}::-webkit-input-placeholder:focus{color: $grayLight !important;}:-moz-placeholder:focus{color: $grayLight !important;}::-moz-placeholder:focus{color: $grayLight !important;}:-ms-input-placeholder:focus{color: $grayLight !important;}
textarea:focus, input[type="text"]:focus, input[type="password"]:focus, input[type="datetime"]:focus, input[type="datetime-local"]:focus, input[type="date"]:focus, input[type="month"]:focus, input[type="time"]:focus, input[type="week"]:focus, input[type="number"]:focus, input[type="email"]:focus, input[type="url"]:focus, input[type="search"]:focus, input[type="tel"]:focus, input[type="color"]:focus, .uneditable-input:focus {   
  -webkit-box-shadow: none; 
  -moz-box-shadow: none; 
  box-shadow: none;  
  outline: 0 none;
}
//Customizing carousel styles
.carousel .carousel-control { visibility: hidden; }
.carousel:hover .carousel-control { visibility: visible; }
.carousel-control{
  top: 50%;
  border: none;
  @include border-radius(0);
  @include transition(all .3s ease);
}
.carousel-fade {
  .carousel-inner {
    .item {opacity: 0;-webkit-transition-property: opacity;-moz-transition-property: opacity;-o-transition-property: opacity;transition-property: opacity;}
    .active {opacity: 1;}
    .active.left,.active.right{left: 0;opacity: 0;z-index: 1;}
    .next.left,.prev.right {opacity: 1;}
  }
  .carousel-control {z-index: 2;}
}
/* END Bootstrap class overrides */

/* Misc */
.g-center{display: block;float: none;margin-left: auto;margin-right: auto;}
.g-floatRight{@extend .pull-right;}
.g-floatLeft{@extend .pull-left;}
.g-clearfix{@extend .clearfix;}
.g-mb25{margin-bottom: 25px;}
.g-ml25{margin-left: 25px;}
.g-pad25{padding: 25px;}
/* END Misc */

/* Header */
.b-header{padding-top: 25px; @extend .clearfix;}    
.b-header__full-row{@extend .g-span12;}
.b-logo{float: left;}
.b-logo__title{
  cursor: pointer;
  display: inline-block;
  font-weight: bold;
  color: white; 
  overflow: hidden;
  padding: 30px 30px;
  text-transform: uppercase;
  @include transition(all .3s ease);       
}
/* END Header */

/* Social media list*/
.b-social-list{@extend .inline;margin-top:37px;margin-left:50px !important;}
.b-social-list--about{@extend .inline;margin-top:0 !important;margin-left:0 !important;}    
.b-social-list--header, .b-social-list--footer{float: right;}
.b-social-list__item{
  display:inline-block;
  width:46px;
  font-size: 23px;
  text-align: center;
  color: white;
  cursor: pointer;
  text-decoration: none;
  padding-top: 10px;
  padding-bottom: 10px; 
  padding-left:0 !important; 
  padding-right:0 !important;
  @include transition(all .3s ease);
}
.b-social-list__icon, .b-social-list__icon:hover{color: white !important;text-decoration: none;}
/* END Social Media list*/

/* Content containers */
.b-container{@extend .container;}
.b-main-cols{margin-top: 25px;} 
.b-main-cols__content-wrapper, .b-main-cols__media-wrapper{@extend .g-span6;} 
/* END Content containers */

/* h2 Headers */
.b-message__header, .b-contacts__header{
  font-size: $baseFontSize*1.57142857; //22px;
  letter-spacing: 1px;
  line-height: $baseLineHeight*1.5; //30px;
  margin-top: 0 !important;
}
/* END h2 Headers */

/* Newsletter */
.b-newsletter__email{
  padding: 11px 19px !important;
  font-size: $baseFontSize*1.14285714 !important; //16px 
  -webkit-background-clip: padding-box; /* for Safari */
  background-clip: padding-box; /* for IE9+, Firefox 4+, Opera, Chrome */
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none; 
  outline: none;
  border-width: 2px !important;
  width: 50%;
  margin-bottom: 0 !important;
}
.b-newsletter__email--error{border-color:red !important;}
.b-newsletter__submit{
  @extend .btn;
  @extend .btn-primary;
  @extend .btn-large;
  @include transition(all .3s ease);
}
.b-newsletter__valid-note{margin-top:5px;font-style:italic;}
/* END Newsletter */

/* Contacts */
.b-contacts--footer{
  .b-contacts__item{
    float: none;
  }
  .b-contacts__list{
    margin-top: 0;
  }
  .b-contacts__text{
    font-size: $baseFontSize*1.57142857; //22px;
    line-height: 40px;
  }
  .b-contacts__text--spacing{
    letter-spacing: .1em
  } 
}
.b-contacts__list{
  list-style-type: none;
  margin-left: 0;
  margin-top: 20px;
  overflow:hidden;
}
.b-contacts__item{line-height:1.2em;float:left;}
#double li{ width:50%;}
.b-contacts__icon{position: relative;}

/* END Contacts */
 
/* Media block */  
.b-media__image{padding-bottom: 25px;}
/* END Media block */ 

/* Footer block */ 
.b-footer{z-index: 2000;}
.b-footer__content{min-height: 100px;}
.b-footer__full-row{@extend .g-span12;}
/* END Footer block */ 

/* Dark and white text themes for pages
   with the picture backgrounds */
.b-dark-pic_container{
  color: white;
}
.b-light-pic_container{
  .b-main-cols__content-wrapper {
    background-color: rgba(255,255,255,0.6);  
    padding-left: 30px;
    padding-right: 30px;
    padding-top: 16px;
  }
  .b-header{padding-top: 15px !important;}
}
.b-footer-pic{background-color: white !important;color: $blackText;} 
/* END Dark and white text themes for pages
   with the picture backgrounds */

/* Landing page services block */
.b-services__container{@extend .b-container;padding-top: 50px;padding-bottom: 50px;}
.b-services__title-row{@extend .g-span12;text-align:center;}
.b-services__item--one-fourth{@extend .g-span3;padding-bottom:25px;text-align:center;}
.b-services__icon{
  color:white;
  cursor:pointer;
  font-size:80px;
  width:80px;
  display:inline-block;
  padding:30px;
  @include transition(all .3s ease);
}
.b-services__ready-title{font-size: 20px; line-height: 40px; font-weight: bold;}
.b-services__contact-link{@extend .b-newsletter__submit;}
/* END Landing page services block */

/* Landing page about block */
.b-about__container{@extend .b-container;padding-top: 50px;}
.b-about__title-row{@extend .g-span12; text-align: center;}
.b-about__item--one-fourth{@extend .g-span3; text-align:center;padding-bottom:50px;}
.b-about__item-content{padding-left:10px;padding-right:10px;}
.b-about__name{font-weight:normal;font-size:$baseFontSize*1.57142857;/*22px;*/}
.b-about__position{display:inline-block;padding-top:5px;padding-bottom:5px;padding-left:10px;padding-right:10px;}
/* END Landing page about block */

/* Landing page about block */
.b-testimonials__container{@extend .b-container;padding-top:50px;padding-bottom:50px;}
.b-testimonials__title-row{@extend .g-span12;text-align:center;}
.b-testimonials__row{@extend .g-span12;}
.b-testimonials__text-container{@extend .g-span8;}
.b-testimonials__pic-container{@extend .g-span4;}
.b-testimonials__text{font-size:$baseFontSize*1.28571429;/*18px*/color:$testimonial;padding-left:25px;padding-right:25px; padding-bottom: 25px;}
.b-testimonials__pic{max-width:150px;height:auto !important;height:150px; padding-bottom: 25px;}
.b-testimonials__name{font-size:$baseFontSize*1.28571429;/*18px*/padding-right:25px;padding-left:25px;}
/* END Landing page about block */

/* Landing page contact us block */
.b-contact-us__title-row{@extend .g-span12;text-align:center;}
.b-contact-us__container{@extend .b-container;padding-top:50px;}
.b-contact-us__one-half{@extend .g-span6;padding-bottom:50px;}
/* END Landing page contact us block */

/* Landing page contact form */
.b-contact-form__input, .b-contact-form__textarea{@extend .b-newsletter__email;width: 80%;}
.b-contact-form__submit{@extend .b-newsletter__submit;width: 30%; display: block; margin: auto;}
.b-contact-form__input, .b-contact-form__textarea{display:block !important;margin:auto;}
/* END Landing page contact form */

/* Responsive styles */
@media (max-width: 767px) { 
  body {padding-left:0;padding-right:0;}
  .b-container {padding-left:20px;padding-right:20px;}
  .b-logo, .b-social-list--header{display: block;float: none;margin-left: auto;margin-right: auto;}
}
