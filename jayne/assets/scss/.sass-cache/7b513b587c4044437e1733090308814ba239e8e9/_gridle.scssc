/**
 * Init gridle
 * Creating the silent classes with user configuration :
 */
%gridle-push-pull-debug-background-common {
	background-size:50px 90%;
	background-position:0 50%;
	background-repeat:repeat-x;
}
%gridle-push-pull-common {
	position:relative;
}
%gridle-container-common {
	width:100%;

	/* For modern browsers */
	&:before,
	&:after {
	    content:"";
	    display:table;
	}
	&:after {
	    clear:both;
	}
	/* For IE 6/7 (trigger hasLayout) */
	& {
	   zoom:1;
	}
}
%gridle-container-debug-common {
	background-color:#f5f5f5;

	&:before,
	&:after {
		content:"";
		display:block;
		background-image: url(data:image/png;base64,);
		height:inherit;
		text-align:center;
		color:white;
		font-size:11px;
		@include box-sizing(border-box);
	}
}
%gridle-grid-debug-common {
	&:before,
	&:after {
		content:"";
		display:block;
		background-image: url(data:image/png;base64,);
		margin:10px 0;
		padding:5px 0;
		text-align:center;
		color:white;
		font-size:11px;
		@include box-sizing(border-box);
	}
	background-color:#daeff5;
}
%gridle-grid-common {
	display:inline-block;
	position:relative;
	@include box-sizing(border-box);
}
%gridle-prefix-debug-common {
	background-color:#dae7e9 !important;
}
%gridle-suffix-debug-common {
	background-color:#dae7e9 !important;
}


/**
 * Common css that cannot be extended cause of variables :
 */
@mixin _gridle_grid_common() {
	@extend %gridle-grid-common;
	@if $gridle-direction == rtl {
		float:right;
		direction:rtl;
	} @else {
		float:left;
		direction:ltr;
	}
	// set padding :;
	padding:0 $gridle-gutter-width/2;
	@if $gridle-debug == true {
		@extend %gridle-grid-debug-common;
	}
}
@mixin _gridle_container_common() {
	@extend %gridle-container-common;
	// debug part :
	@if ($gridle-debug == true) {
		@extend %gridle-container-debug-common;
		&:before {
			@if $gridle-debug-show-class-names == true {
				padding:10px $gridle-gutter-width/2;
				content:"container-#{$gridle-columns-count}" !important;
			}
		}
		&:after {
			@if $gridle-debug-show-class-names == true {
				padding:10px $gridle-gutter-width/2;
				content:"end container-#{$gridle-columns-count}" !important;
			}
		}
	}
}
@mixin _gridle_push_common() {
	// extend common :
	@extend %gridle-push-pull-common;
	@if $gridle-debug == true {
		@extend %gridle-push-pull-debug-background-common;
		background-color:#f4efdf !important;
		@if $gridle-direction == ltr {
			background-image: url(data:image/png;base64,);
		} @else {
			background-image: url(data:image/png;base64,);
		}
	}
}
@mixin _gridle_pull_common() {
	@extend %gridle-push-pull-common;
	@if $gridle-debug == true {
		@extend %gridle-push-pull-debug-background-common;
		background-color:#cfe4d5 !important;
		@if $gridle-direction == ltr {
			background-image: url(data:image/png;base64,);
		} @else {
			background-image: url(data:image/png;base64,);
		}
	}
}
@mixin _gridle_prefix_common() {
	@if $gridle-debug == true {
		@extend %gridle-prefix-debug-common;
	}
}
@mixin _gridle_suffix_common() {
	@if $gridle-debug == true {
		@extend %gridle-suffix-debug-common;
	}
}


/**
 * Register an state :
 *
 * @param 	String 	$name 		The state name
 * @param 	int 	$min-width 	The min width
 * @param 	int 	$max-width 	The max width
 */
@mixin gridle_register_state(
	$name,
	$min-width : null,
	$max-width : null
) {
	// register new state :
	@if $_gridle-state-01-name == null {
		$_gridle-state-01-name : $name;
		$_gridle-state-01-min-width : $min-width;
		$_gridle-state-01-max-width : $max-width;
	} @else if $_gridle-state-02-name == null {
		$_gridle-state-02-name : $name;
		$_gridle-state-02-min-width : $min-width;
		$_gridle-state-02-max-width : $max-width;
	} @else if $_gridle-state-03-name == null {
		$_gridle-state-03-name : $name;
		$_gridle-state-03-min-width : $min-width;
		$_gridle-state-03-max-width : $max-width;
	} @else if $_gridle-state-04-name == null {
		$_gridle-state-04-name : $name;
		$_gridle-state-04-min-width : $min-width;
		$_gridle-state-04-max-width : $max-width;
	} @else if $_gridle-state-05-name == null {
		$_gridle-state-05-name : $name;
		$_gridle-state-05-min-width : $min-width;
		$_gridle-state-05-max-width : $max-width;
	} @else if $_gridle-state-06-name == null {
		$_gridle-state-06-name : $name;
		$_gridle-state-06-min-width : $min-width;
		$_gridle-state-06-max-width : $max-width;
	} @else if $_gridle-state-07-name == null {
		$_gridle-state-07-name : $name;
		$_gridle-state-07-min-width : $min-width;
		$_gridle-state-07-max-width : $max-width;
	} @else if $_gridle-state-08-name == null {
		$_gridle-state-08-name : $name;
		$_gridle-state-08-min-width : $min-width;
		$_gridle-state-08-max-width : $max-width;
	} @else if $_gridle-state-09-name == null {
		$_gridle-state-09-name : $name;
		$_gridle-state-09-min-width : $min-width;
		$_gridle-state-09-max-width : $max-width;
	} @else if $_gridle-state-10-name == null {
		$_gridle-state-10-name : $name;
		$_gridle-state-10-min-width : $min-width;
		$_gridle-state-10-max-width : $max-width;
	}
}


/**
 * Register default states :
 */
@mixin gridle_register_default_states() {
	// register mobile and tablet states :
	@include gridle_register_state("mobile",0,400px);
	@include gridle_register_state("tablet",401px,767px);
}


/**
 * Responsive helpers mixins :
 *
 * @param 	String 	$media 	The media name to response for
 */
@mixin gridle_responseTo(
	$media_min-width,
	$max-width : null
) {
	// get the index of state :
	$idx : _gridle_get_media_query_index_by_name($media_min-width);
	// check if the first params is null :
	@if $media_min-width == null {
		@content;
	} @else if $media_min-width == 'print' or $media_min-width == print {
		@if $gridle-body-states-classes { body.print { @content; } }
		@media only print {
			@content;
		}
	} @else if $media_min-width == 'tv' or $media_min-width == tv {
		@if $gridle-body-states-classes { body.tv { @content; } }
		@media only tv {
			@content;
		}
	} @else if $media_min-width == 'portrait' or $media_min-width == portrait {
		@if $gridle-body-states-classes { body.portrait { @content; } }
		@media only screen and (orientation: portrait) {
			@content;
		}
	} @else if $media_min-width == 'landscape' or $media_min-width == landscape {
		@if $gridle-body-states-classes { body.landscape { @content; } }
		@media only screen and (orientation: landscape) {
			@content;
		}
	} @else if $media_min-width == 'retina' or $media_min-width == retina {
		@if $gridle-body-states-classes { body.retina { @content; } }
		@media only screen and (-webkit-min-device-pixel-ratio: 2),
			   only screen and (   min--moz-device-pixel-ratio: 2),
			   only screen and (     -o-min-device-pixel-ratio: 2/1),
			   only screen and (        min-device-pixel-ratio: 2),
			   only screen and (                min-resolution: 192dpi),
			   only screen and (                min-resolution: 2dppx)
		{
			@content;
		}
	} @else if $idx != null {
		// get vars :
		$m : _gridle_get_media_query_vars($idx,"name");
		$min-width : _gridle_get_media_query_vars($idx,"min-width");
		$max-width : _gridle_get_media_query_vars($idx,"max-width");
		// check if exist :
		@if $m != null {
			// process max and min width :
			@if $min-width == null { $min-width : 0; }
			@if $max-width == null { $max-width : 99999px; }
			// write media query :
			@if $gridle-body-states-classes { body.#{$m} { @content; } }
			@media screen and (min-width: $min-width) and (max-width: $max-width) { @content; }
		}
	} @else {
		// check for min and max width :
		@if $media_min-width != null and $max-width != null {
			@media screen and (min-width: $media_min-width) and (max-width: $max-width) { @content; }
		} @else if $media_min-width != null and $max-width == null {
			@media screen and (min-width: $media_min-width) { @content; }
		} @else if $media_min-width == null and $max-width != null {
			@media screen and (max-width: $max-width) { @content; }
		}
	}
}


/**
